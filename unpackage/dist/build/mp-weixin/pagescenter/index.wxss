.custom-map-list{display:#000;position:fixed;left:0;right:0;height:calc(100vh - 260px);bottom:0;background-color:#fff;padding:0 0 0 0;box-sizing:border-box}.custom-map-list-wapper{width:100%;height:calc(100vh - 260px)}.custom-map-list-item{display:flex;align-items:center;position:relative;padding:30rpx 30rpx 30rpx 30rpx;transition:all ease .2s;background-color:#fff}.custom-map-list-item.active{background-color:#f7f7f7}.custom-map-list-item:active{background-color:#f7f7f7}.custom-map-list-item:after{content:"";position:absolute;z-index:0;bottom:0;left:0;width:100%;border-bottom:1px solid #f7f7f7;-webkit-transform:scaleY(.5);transform:scaleY(.5);-webkit-transform-origin:0 0;transform-origin:0 0}.custom-map-list-item-lside{flex:1}.custom-map-list-item-rside{padding-left:10rpx}.custom-map-list-item-rside image{width:42rpx;height:42rpx}.custom-map-list-item-title{font-size:32rpx;color:#333;padding-bottom:8rpx}.custom-map-list-item-desc{font-size:28rpx;color:#999;line-height:1.3}.custom-map-list-empty-page{display:flex;flex-direction:column;justify-content:center;align-items:center;height:calc(100vh - 260px)}.custom-map-list-empty-page image{width:120rpx;height:120rpx;opacity:.3}.custom-map-list-empty-page .text{font-size:24rpx;color:#999;line-height:2;padding-top:30rpx}.confirm-btn{position:fixed;right:40rpx;bottom:80rpx;width:120rpx;height:120rpx;background-image:linear-gradient(45deg,rgba(98,178,100,.6),#62b264);font-size:24rpx;line-height:1.2;color:#fff;border-radius:50%;display:flex;justify-content:center;align-items:center;transition:all ease .3s;box-shadow:0 3px 15px rgba(0,0,0,.2)}.confirm-btn:active{opacity:.8}