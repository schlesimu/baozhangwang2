<view class="cu-form-group video-com"><view class="{{['grid','grid-square','flex-sub','col-'+col]}}"><block wx:for="{{videoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['ViewVideo',['$0'],[[['videoList','',index,'path.tempFilePath']]]]]]]}}" class="bg-img" bindtap="__e"><block wx:if="{{uShow==true}}"><video src="{{item.path.tempFilePath}}" object-fit="cover"></video></block><view class="cu-tag bg-red" style="z-index:20190118;" data-index="{{index}}" data-event-opts="{{[['tap',[['DelVideo',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view><block wx:if="{{item.loading!='100%'}}"><view class="progress-mask flex justify-center align-center padding-lr-sm"><view class="cu-progress bg-mask round xs"><view class="bg-white" style="{{'width:'+(item.loading)+';'}}"></view></view></view></block><block wx:if="{{item.error}}"><view class="progress-mask flex justify-center align-center text-white" data-index="{{index}}" data-event-opts="{{[['tap',[['DelVide',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-roundclose"></text></view></block></view></block><block wx:if="{{videoList.length<max}}"><view data-event-opts="{{[['tap',[['ChooseVideo',['$event']]]]]}}" class="solids" style="width:142rpx;height:142rpx;overflow:visible;" bindtap="__e"><block wx:if="{{$slots.default}}"><slot></slot></block><block wx:else><image class="cuIcon-cameraadd" src="{{IMG_URL+'static/miniwechat/creame_gray.png'}}" mode></image></block></view></block></view><block wx:if="{{showBig}}"><view class="flex justify-center align-center" style="position:fixed;top:0;left:0;z-index:20140223;background-color:rgba(0,0,0,.7);width:100%;height:100%;"><view data-event-opts="{{[['tap',[['closeVideo',['$event']]]]]}}" class="cuIcon-close text-white bold" style="position:absolute;top:30rpx;right:0;z-index:20190118;width:96rpx;height:96rpx;" bindtap="__e"></view><video style="width:100%;height:100%;" src="{{currentVideo}}" autoplay="{{true}}"></video></view></block></view>