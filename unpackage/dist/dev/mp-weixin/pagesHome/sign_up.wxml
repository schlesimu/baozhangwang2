<view class=" padding"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" class=" " bindsubmit="__e"><view class="head_list cu-list menu sm-border"><view class="cu-item arrow"><view class="flex align-center justify-between " style="width:100%;"><view style="width:100%;"><view class="text-gray margin-bottom-sm" style="font-size:13px;">姓名</view><view class="other" style="{{(nickname?'color:#333;font-weight: bold;':'color:#e5e5e5')}}"><input style="{{(nickname?'color:#333;font-weight:bold':'color:#e5e5e5')}}" type="text" name="user_nickname" placeholder="请输入真实姓名" placeholder-style="color:#e5e5e5;" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]],['nicknameInput',['$0'],['nickname']]]],['blur',[['nickblur',['$0'],['nickname']]]]]}}" value="{{nickname}}" bindinput="__e" bindblur="__e"/></view></view></view></view><view class="cu-item arrow"><view class="flex align-center justify-between " style="width:100%;"><view class="text-gray margin-bottom-sm" style="font-size:13px;">性别</view><view class="flex align-center justify-start"><block wx:for="{{sex}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['chooseSex',['$0',index],[[['sex','',index]]]]]]]}}" class="{{['sexTag',sexIndex==item.value?'active':'']}}" bindtap="__e">{{''+item.label+''}}</view></block></view></view></view><view class="cu-item arrow"><view class="flex align-center justify-between "><view><view class="text-gray margin-bottom-sm" style="font-size:13px;">手机号码</view><view class="other" style="{{(phone?'color:#333;font-weight: bold;':'color:#e5e5e5')}}"><input style="{{(phone?'color:#333;font-weight:bold':'color:#e5e5e5')}}" type="number" maxlength="11" name="phone" placeholder="请输入手机号" placeholder-style="color:#e5e5e5" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view></view></view></view><block wx:for="{{img}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['chooseImg',[index]]]]]}}" class="card_item" bindtap="__e"><view><view class=" margin-tb-sm"><image class="bg_img" src="{{item.pic||IMG_URL+'static/miniwechat/cardbg.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['yulan',['$0'],[[['img','',index,'pic']]]]]]]}}" catchtap="__e"></image><view class="txt-pop">选填</view><view hidden="{{!(item.pic=='')}}" class="btn_chuan "><view class="btn_center flex align-center justify-center"><image src="{{IMG_URL+'static/miniwechat/xiangji@2x.png'}}" mode="aspectFill"></image><view>{{item.content}}</view></view></view></view><view class="f24 text-gray">{{'说明：'+item.desc+''}}</view></view></view></block><view class="list_item flex align-center justify-between"><view class="color6 f28">是否发送简历</view><view class="flex align-center"><view class="mr10">是</view><switch class="{{['deepBlue',checked?'checked':'']}}" style="transform:scale(0.7);" checked="{{checked}}" data-event-opts="{{[['change',[['switch1Change',['$event']]]]]}}" bindchange="__e"></switch></view></view><view><button class="btn_next bg_deep" style="height:88rpx;margin-bottom:80rpx;" form-type="submit">确认报名</button></view></view></form></view>