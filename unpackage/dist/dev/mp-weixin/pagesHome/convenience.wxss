.title {
  font-size: 32rpx;
  font-weight: bold;
  line-height: 1.2;
  letter-spacing: 1;
  margin-bottom: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  line-clamp: 2;
}
.picitem {
  width: 215rpx;
  height: 152rpx;
  border-radius: 20rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.picitem image {
  width: 100%;
  height: 100%;
}
.btn_btn {
  width: 77rpx;
  height: 32rpx;
  background: #50aeff;
  border: 1px solid #50aeff;
  color: #ffffff;
  text-align: center;
  line-height: 32rpx;
  font-size: 11px;
}
.picitem:nth-child(3) {
  margin-right: 0;
}
.inputBox {
  width: 100%;
  height: 58rpx;
  border-radius: 34rpx;
  border: 1px solid #ddd;
}
.top_img {
  position: fixed;
  right: 40rpx;
  bottom: 140rpx;
  width: 88rpx;
  height: 88rpx;
}
.fixed {
  position: fixed;
  top: 0;
  width: 100%;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 100;
  padding: 20rpx 30rpx;
}
.type_item {
  padding: 6rpx 20rpx;
  background: white;
  border: 1px solid #e6e6e6;
  border-radius: 4rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.up_margin {
  margin: auto;
  width: 686rpx;
  position: relative;
  background-color: #fff;
  z-index: 3;
  border-radius: 16rpx;
}
.worktime-tab {
  text-align: center;
  border-bottom: 1px solid #efefef;
}
.worktime-tab .worktime-tab-item {
  width: 100%;
  height: 72rpx;
}
.worktime-tab .current-tab {
  border-bottom: 3px solid #50aeff;
  color: #50aeff;
  padding-bottom: 10rpx;
}
.worktime-tab .current-tab .current-tab-font {
  font-weight: 700;
}
.uni-margin-wrap {
  width: 150%;
  position: relative;
  top: 50rpx;
  z-index: 99;
}
.swiper {
  height: 80rpx;
}
.swiper-item {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  justify-content: flex-start;
  align-items: center;
}
.kailong {
  position: absolute;
  right: -16rpx;
  top: 16rpx;
  width: 0;
  height: 0;
  border-right: 14rpx solid transparent;
  border-left: 14rpx solid transparent;
  border-top: 14rpx solid #333;
}
.kaiact {
  position: absolute;
  right: -16rpx;
  top: 16rpx;
  width: 0;
  height: 0;
  border-right: 14rpx solid transparent;
  border-left: 14rpx solid transparent;
  border-top: 14rpx solid #999;
}

