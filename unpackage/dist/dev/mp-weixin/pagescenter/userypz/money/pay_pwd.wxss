page {
  height: 100%;
  background: #fff;
}
.btn_wc {
  width: 338rpx;
  height: 68rpx;
  background: #ff3134;
  border-radius: 34rpx;
  text-align: center;
  line-height: 68rpx;
  color: #ffffff;
  margin: auto;
}
.content {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  background: #fff;
  color: #555;
  height: 100%;
  font-weight: 400;
}
.content .keypan {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
}
.content .keypan .titles {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  padding: 0 5%;
  font-size: 32rpx;
  background: #fff;
  color: #292824;
}
.content .keypan .titles ._span {
  width: 100rpx;
  height: 70rpx;
  display: inline-block;
  padding-left: 30rpx;
}
.content .keypan .keyNumbers {
  display: flex;
  flex-wrap: wrap;
}
.content .keypan .pan_num_key {
  width: 33.33%;
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  background: #fff;
  font-size: 48rpx;
  border: 1rpx solid #F7F7F7;
}
.content .keypan .pan_num_key.gray {
  background: #ECECEC;
}
.content .keypan .pan_num_key:active {
  background: #EEEEEE;
  -webkit-transform: all .5s;
          transform: all .5s;
}
.content .navigation_bar {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 60rpx;
}
.content .navigation_bar .back-icon {
  width: 18rpx;
  height: 34rpx;
}
.content .login {
  width: 100%;
  height: 100%;
  background: #fff;
}
.content .login .l_top {
  width: 100%;
  margin: 150rpx auto 0;
  position: relative;
  background: #fff;
}
.content .login .l_top .l_text {
  width: 445rpx;
  height: 69rpx;
  font-size: 56rpx;
  font-weight: bold;
  text-align: center;
  margin: auto;
}
.content .login .l_top .fangshi {
  font-size: 30rpx;
  color: #666666;
  margin-top: 20rpx;
}
.content .login .l_top .lt {
  margin-top: -20rpx;
  font-size: 25rpx;
  color: #555;
}
.content .title {
  color: #616161;
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 34rpx;
}
.content .mima {
  flex-direction: row;
  width: 100%;
  height: 96rpx;
  margin: 60rpx auto 0;
  display: flex;
  justify-content: center;
  position: relative;
}
.content .mima .item {
  width: 79rpx;
  height: 79rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 20rpx;
  background-color: #F2F2F2;
}
.content .mima .item .line {
  width: 2rpx;
  height: 40rpx;
  background: #979797;
  -webkit-animation: shan 1s ease infinite;
          animation: shan 1s ease infinite;
}
.content .mima .item .dot {
  width: 20rpx;
  height: 20rpx;
  border-radius: 20rpx;
  background: black;
}
.content .mima .item:nth-child(-n+5) {
  border-right: none;
}
.content .mima .trade_pwd {
  position: absolute;
  height: 78rpx !important;
  width: 480rpx;
  opacity: 0;
}
@-webkit-keyframes shan {
from {
    opacity: 1;
}
to {
    opacity: 0;
}
}
@keyframes shan {
from {
    opacity: 1;
}
to {
    opacity: 0;
}
}
@-webkit-keyframes checked_bg {
0% {
    background: #fff;
}
50% {
    background: #D3D1E2;
}
100% {
    background: #fff;
}
}
@keyframes checked_bg {
0% {
    background: #fff;
}
50% {
    background: #D3D1E2;
}
100% {
    background: #fff;
}
}

