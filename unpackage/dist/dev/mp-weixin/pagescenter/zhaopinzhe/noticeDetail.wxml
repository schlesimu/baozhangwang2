<view class="padding"><view class="title overflow-two">{{plant_detail.title}}</view><view class="text-gray text-sm mb10">{{plant_detail.update_time}}</view><view data-event-opts="{{[['tap',[['copy',['$0'],['plant_detail.content']]]]]}}" class="text-df mtb10" bindtap="__e"><text>{{''+plant_detail.content+''}}</text></view><view class="mb10"><block wx:if="{{plant_detail.driver_name!=''&&plant_detail.driver_phone!=''}}"><view class="f30 gray6 mb5">{{''+(plant_detail.driver_name||'')+'：'}}<text style="color:#007AFF;" selectable="true" data-event-opts="{{[['tap',[['call',['$0'],['plant_detail.driver_phone']]]]]}}" bindtap="__e">{{plant_detail.driver_phone||''}}</text></view></block><block wx:if="{{plant_detail.resident_manager_name!=''&&plant_detail.resident_manager_phone!=''}}"><view class="f30 gray6 mb5">{{''+(plant_detail.resident_manager_name||'')+'：'}}<text style="color:#007AFF;" selectable="true" data-event-opts="{{[['tap',[['call',['$0'],['plant_detail.resident_manager_phone']]]]]}}" bindtap="__e">{{plant_detail.resident_manager_phone||''}}</text></view></block><block wx:if="{{plant_detail.finance_manager_name!=''&&plant_detail.finance_manager_phone!=''}}"><view class="f30 gray6">{{''+(plant_detail.finance_manager_name||'')+'：'}}<text style="color:#007AFF;" selectable="true" data-event-opts="{{[['tap',[['call',['$0'],['plant_detail.finance_manager_phone']]]]]}}" bindtap="__e">{{plant_detail.finance_manager_phone||''}}</text></view></block></view><block wx:for="{{plant_detail.url}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['yulan',['$0'],[[['plant_detail.url','',index]]]]]]]}}" style="background-color:#F7F7F7;" bindtap="__e"><image style="width:100%;" src="{{item.url}}" mode="widthFix"></image></view></block></view>