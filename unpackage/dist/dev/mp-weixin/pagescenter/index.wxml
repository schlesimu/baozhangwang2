<view class="page-wapper"><view id="map"></view><view class="custom-map-list"><scroll-view class="custom-map-list-wapper" scroll-y="{{true}}"><view><block wx:if="{{showCurrentAddress}}"><view data-event-opts="{{[['tap',[['handleChooseAddressListItem',[0]]]]]}}" class="{{[(true)?'custom-map-list-item':'',(selectAddressIndex==0)?'active':'']}}" bindtap="__e"><view class="custom-map-list-item-lside"><view class="custom-map-list-item-title">当前位置</view><view class="custom-map-list-item-desc">{{currentAddress.name}}</view></view><block wx:if="{{selectAddressIndex==0}}"><view class="custom-map-list-item-rside"></view></block></view></block><block wx:for="{{addressSearchResult}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['handleChooseAddressListItem',[index+1]]]]]}}" class="{{[(true)?'custom-map-list-item':'',(selectAddressIndex==index+1)?'active':'']}}" bindtap="__e"><view class="custom-map-list-item-lside"><view class="custom-map-list-item-title">{{item.name}}</view><view class="custom-map-list-item-desc">{{item.district+item.address}}</view></view><block wx:if="{{selectAddressIndex==index+1}}"><view class="custom-map-list-item-rside"></view></block></view></block><block wx:if="{{!showCurrentAddress&&addressSearchResult.length==0}}"><view class="custom-map-list-empty-page"><image src="/static/choose_address_empty.png"></image><view class="text">未检索到地址~</view></view></block></view></scroll-view></view><view data-event-opts="{{[['tap',[['handleConfirmChooseAddress',['$event']]]]]}}" class="confirm-btn" bindtap="__e"><text>确定\n选择</text></view></view>