page {
  background-color: #f7f7f7 !important;
}
.my {
  position: relative;
  height: 100%;
  background: #f7f7f7;
  padding-bottom: 100rpx;
}
.radius {
  border-radius: 16rpx;
}
.top_section {
  height: 510rpx;
  width: 100%;
  padding-bottom: 90rpx;
}
.top_section .bg_color {
  width: 100%;
  height: 417rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.bg_color > image {
  width: 100%;
  height: 100%;
}
.up_margin {
  margin: auto;
  width: 686rpx;
  position: relative;
  background-color: #fff;
  z-index: 3;
  border-radius: 16rpx;
}
.cu-list.menu {
  border-radius: 16rpx;
}
.userInfo {
  padding: 20rpx 30rpx 0;
}
.userInfo .cu-tag {
  border-radius: 6px;
  color: #fff;
  height: 36rpx;
  background: linear-gradient(136deg, #fce38f 0%, #f2a457 100%);
}
.dir {
  color: #999999;
}
.tui-msg-box image {
  width: 160rpx;
  height: 160rpx;
}
.tui-msg-pic {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin-right: 24rpx;
}
.tui-msg-name {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 40rpx;
  font-weight: bold;
}
.my_bgImg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 431rpx;
}
.cu-item {
  padding: 30rpx 30rpx;
  font-size: 32rpx;
}
.cu-item image {
  margin-right: 20rpx;
}
.nav_btns {
  padding: 36rpx 30rpx 0;
  position: relative;
  z-index: 3;
  padding-right: 240rpx;
}
.nav_btns .item {
  font-size: 48rpx;
  margin-top: 40rpx;
  margin-left: 20rpx;
}
.nav_btns .item image {
  width: 42rpx;
  height: 42rpx;
}
.order {
  width: 44rpx;
  height: 44rpx;
}
.path_list {
  padding-top: 0;
  position: relative;
  z-index: 999;
  width: 686rpx;
  margin: auto;
  border-radius: 16rpx;
  margin-top: 60rpx;
  margin-bottom: 140rpx;
  background-color: #fff;
}
.path_list .text-grey {
  font-size: 30rpx;
}

