.box_item {
  width: 50%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}
.box_left {
  color: #1b90ff;
  position: relative;
}
.box_left::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 54rpx;
  height: 6rpx;
  z-index: 99;
  background-color: #1b90ff;
}
.sticky-box {
  position: -webkit-sticky;
  position: sticky;
  z-index: 99;
  background: #fff;
}
.choosed {
