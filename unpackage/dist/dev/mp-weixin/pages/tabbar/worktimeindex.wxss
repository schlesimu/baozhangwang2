page {
  background-color: #f7f7f7 !important;
}
.my {
  position: relative;
  height: 100%;
  background: #f7f7f7;
}
.worktime-top-section {
  width: 100%;
}
.worktime-top-section .bg_color {
  width: 100%;
  height: 417rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.bg_color > image {
  width: 100%;
  height: 100%;
}
.up_margin {
  margin: auto;
  width: 686rpx;
  position: relative;
  background-color: #fff;
  z-index: 3;
  border-radius: 16rpx;
}
.tui-msg-box {
  width: 686rpx;
  margin: 0 auto;
}
.tui-msg-box image {
  width: 160rpx;
  height: 160rpx;
}
.tui-msg-pic {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin-right: 24rpx;
}
.path_list {
  padding-top: 0;
  position: relative;
  z-index: 999;
  width: 686rpx;
  margin: auto;
  border-radius: 16rpx;
  margin-top: 60rpx;
  margin-bottom: 140rpx;
  background-color: #fff;
}
.path_list .text-grey {
  font-size: 30rpx;
}
.worktime-tab {
  text-align: center;
}
.worktime-tab .worktime-tab-item {
  width: 100%;
  height: 72rpx;
}
.worktime-tab .current-tab {
  border-bottom: 3px solid #50aeff;
  color: #50aeff;
  padding-bottom: 10rpx;
}
.worktime-tab .current-tab .current-tab-font {
  font-weight: 700;
}

