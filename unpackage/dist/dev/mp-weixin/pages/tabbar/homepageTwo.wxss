page {
  background-color: #fff;
  height: 100%;
}
.bg_img {
  width: 106rpx;
  height: 106rpx;
  margin-right: 20rpx;
  background-color: #f7f7f7;
}
.bg_img image {
  width: 100%;
  height: 100%;
}
.box_sort {
  width: 97rpx;
  height: 90rpx;
  border: 1px solid #cccccc;
  text-align: center;
  line-height: 45rpx;
}
.box_sort view {
  font-size: 12px;
}
.active {
  color: #1b90ff;
  border-bottom: 2px solid #1b90ff;
}
.btn_top {
  padding: 2rpx 15rpx;
  background: #1b90ff;
  border-radius: 8rpx;
  color: #ffffff;
  font-size: 12px;
  text-align: center;
}
.returnBox {
  min-width: 97rpx;
  height: 87rpx;
  background: white;
  border: 1px solid #cccccc;
  opacity: 1;
  border-radius: 4rpx;
  box-sizing: border-box;
}
.returnBox .top_blue {
  height: 50%;
  background-color: #50aeff;
  margin-top: -3rpx;
  color: #ffffff;
  width: 100%;
}
.left {
  background-color: #ffffff;
  color: #1b90ff;
  border: 1px solid #1b90ff;
}
.cu-list {
  top: 100rpx;
}
.cu-list.menu {
  padding: 0 30rpx;
}
.cu-list.menu > .cu-item .content > uni-image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}

