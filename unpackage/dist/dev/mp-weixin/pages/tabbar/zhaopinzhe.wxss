page {
  background-color: #f7f7f7 !important;
}
.my {
  position: relative;
  background: #f7f7f7;
  height: 100%;
}
.radius {
  border-radius: 20rpx;
  overflow: hidden;
}
.top_section {
  min-height: 385rpx;
  padding-bottom: 90rpx;
}
.top_section .bg_color {
  width: 100%;
  height: 375rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.bg_color > image {
  width: 100%;
  height: 100%;
}
.up_margin {
  margin: auto;
  margin-top: -64rpx;
  width: 686rpx;
  height: 154rpx;
  border-radius: 20rpx;
  position: relative;
  background-color: #fff;
  z-index: 3;
}
.userInfo {
  padding: 20rpx 30rpx 0;
}
.userInfo .cu-tag {
  border-radius: 4px;
  color: #5a4015;
  height: 36rpx;
  background: #ffb417;
}
.userInfo .cu-tag image {
  width: 23rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.dir {
  color: #999999;
}
.tui-msg-box {
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: space-between;
}
.tui-msg-pic {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #fff;
}
.tui-msg-item {
  color: #ffffff;
}
.contant {
  width: 686rpx;
  margin: auto;
  border-radius: 16rpx;
  background-color: #ffffff;
}
.contant image {
  margin-right: 10rpx;
}
.nav_btns {
  padding: 20rpx 30rpx;
  position: relative;
  z-index: 3;
  padding-right: 240rpx;
}
.nav_btns .item {
  font-size: 48rpx;
  margin-left: 40rpx;
  margin-top: 40rpx;
}
.nav_btns .item image {
  width: 42rpx;
  height: 38rpx;
}
.nav_btns .cu-avatar {
  background: transparent;
}
.userMsg .cu-item {
  background-color: transparent;
}
/* 浮动导航 */
.float-nav {
  background: #fff;
}
.float-nav image {
  width: 45rpx;
  height: 45rpx;
}
.float-nav .txt {
  margin-top: 10rpx;
  font-size: 14px;
}
.myOrder .title {
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}
.myOrder image {
  width: 50rpx;
  height: 50rpx;
}
.myOrder .dir {
  font-size: 12px;
}
.fonticon {
  font-size: 34rpx;
}
.grade {
  margin-right: -30rpx;
  background: #fff;
  padding: 12rpx;
  border-top-left-radius: 50px;
  border-bottom-left-radius: 50px;
}
.grade .dengji {
  width: 44rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
}
.grade.level1 {
  color: #52eccb;
  font-size: 12px;
}
.path_list {
  padding-top: 0;
}
.path_list .text-grey {
  font-size: 30rpx;
}
.gongsi {
  width: 686rpx;
  height: 273rpx;
  border-radius: 30rpx;
  background-color: #fff;
  margin: -63rpx 32rpx 0rpx;
}
.gongsi .user_img {
  width: 143rpx;
  height: 143rpx;
  margin: 30rpx 24rpx 0 0;
  border-radius: 10rpx;
}
.gongsi .user_img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.gongsi .btn {
  padding: 5rpx 15rpx;
  background-color: #fa6b5a;
  border-radius: 22rpx;
  color: #fff;
}
.gongsi .user_box {
  width: 143rpx;
  height: 143rpx;
  border-radius: 10rpx;
  border: 1rpx solid #999999;
  text-align: center;
  margin-top: 30rpx;
  padding: 23rpx 0;
}
.cu-list.menu > .cu-item .content > uni-image {
  width: 98rpx;
  height: 98rpx;
}

