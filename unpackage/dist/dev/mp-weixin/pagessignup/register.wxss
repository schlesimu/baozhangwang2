@font-face {
  font-family: 'HMfont-login';
  src: url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2");
}
.icon {
  font-family: 'HMfont-login' !important;
  font-style: normal;
  color: #ffffff;
}
.icon.weixin:before {
  content: '\e615';
}
.icon.qq:before {
  content: '\e752';
}
.icon.weibo:before {
  content: '\e63d';
}
.icon.xiaomi:before {
  content: '\e661';
}
page {
  background: #fff;
  height: calc(100%-StatusBar);
}
.header {
  width: 686rpx;
  margin: auto;
}
.header .logo {
  font-size: 48rpx;
  letter-spacing: 3rpx;
  margin-bottom: 20rpx;
  font-weight: bold;
}
.header .phone {
  color: #c2c2c2;
  font-size: 24rpx;
}
.form .code {
  position: relative;
}
.form .code .get-code {
  position: absolute;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  right: 0;
  z-index: 3;
}
.form .codestart {
  padding: 0 40rpx 0 0;
  position: relative;
}
.form .codestart::after {
  position: absolute;
  right: -16rpx;
  top: 16rpx;
  width: 0;
  height: 0;
  border-right: 14rpx solid transparent;
  border-left: 14rpx solid transparent;
  border-top: 14rpx solid #666;
}
.form {
  margin-top: 0rpx;
  width: 686rpx;
  font-size: 30rpx;
  margin: auto;
}
.form .username,
.form .password,
.form .code {
  width: calc(100%);
  height: 90rpx;
  display: flex;
  align-items: center;
  margin-bottom: 26rpx;
}
.form .username input,
.form .password input,
.form .code input {
  width: 100%;
  height: 90rpx;
  font-size: 32rpx;
  letter-spacing: 2rpx;
}
.form .username .icon_phone,
.form .password .icon_phone,
.form .code .icon_phone {
  width: 29rpx;
  height: 38rpx;
}
.oauth {
  position: absolute;
  bottom: 50rpx;
  width: 100%;
}
.oauth .text {
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: rgba(255, 255, 255, 0.8);
  font-size: 28rpx;
}
.oauth .list {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx 0;
}
.oauth .list .icon {
  font-size: 80rpx;
  margin: 0 30rpx;
}
.blue-yy {
  color: #cccccc;
}
.userfooter-yy {
  font-size: 22rpx;
  color: #666666;
}
.res_bom {
  position: absolute;
  bottom: 20%;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.res_bom .quick_font {
  display: block;
  font-size: 14px;
  color: #999999;
  margin-top: 14rpx;
}
.res_bom image {
  width: 100rpx;
  height: 100rpx;
}

